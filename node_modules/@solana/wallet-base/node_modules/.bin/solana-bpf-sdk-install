#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/bash" ]; then
  "$basedir/bash"  "$basedir/../@solana/web3.js/bin/bpf-sdk-install.sh" "$@"
  ret=$?
else 
  bash  "$basedir/../@solana/web3.js/bin/bpf-sdk-install.sh" "$@"
  ret=$?
fi
exit $ret
